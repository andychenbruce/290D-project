
[bruh_idk
  ((x : int), (y : int), (z : int)) -> bool
  (((let a = 2),
   (let c = 3)) in
   ((((x + c) - a) + (y + z)) > a))];

[is_greater_than_2
  ((x : int)) -> bool
  ((x > 4))];

[test ((x : int)) -> bool
  (is_greater_than_2(x))];

[is_less_than_or_equal_to_2 ((x : int)) -> bool
  (!is_greater_than_2(x))];

{is_greater_than_2(3)};

{test(4)};

{!is_greater_than_2(2)};

{!is_greater_than_2(1)};

{!is_less_than_or_equal_to_2(4)};

[pow2 ((x : int)) -> int
  (fold (0, x) (acc, 1) (acc * 2))
];