[bruh_idk
  ((x : int), (y : int), (z : int)) -> bool
  (((let a = 2),
   (let c = 3)) in
   ((((x + c) - a) + (y + z)) > a))];

[is_greater_than_2
  ((x : int)) -> bool
  ((x > 4))];

[test ((x : int)) -> bool
  (is_greater_than_2(x))];

[is_less_than_or_equal_to_2 ((x : int)) -> bool
  (!is_greater_than_2(x))];

[hard_eq_test
  ((x : int), (y : int)) -> bool
  (__eq(x, y))];

[soft_eq_test
  ((x : int), (y : int)) -> bool
  (__eq(x, y))];

{is_greater_than_2(3)};
{test(4)};
{!is_greater_than_2(2)};
{!is_greater_than_2(1)};
{!is_less_than_or_equal_to_2(4)};

{hard_eq_test(5, 5)};   // True
{hard_eq_test(5, 4)};   // False

{soft_eq_test(5, 5)};   // ~1.0
{soft_eq_test(5, 4)};   // <1.0
